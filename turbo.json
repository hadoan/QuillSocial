    "TWILIO_VERIFY_SID",
    "UPSTASH_REDIS_REST_TOKEN",
    "UPSTASH_REDIS_REST_URL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "VITAL_API_KEY",
    "VITAL_DEVELOPMENT_MODE",
    "VITAL_REGION",
    "VITAL_WEBHOOK_SECRET",
    "ZAPIER_INVITE_LINK",
    "ZOHOCRM_CLIENT_ID",
