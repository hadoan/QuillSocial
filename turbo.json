{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    "yarn.lock"
  ],
  "tasks": {
    "@quillsocial/prisma#build": {
      "cache": false
    },
    "@quillsocial/prisma#db-migrate": {
      "cache": false,
      "dependsOn": [
        "@quillsocial/prisma#db-up"
      ],
      "inputs": [
        "./schema.prisma",
        "./migrations/**/*.sql"
      ]
    },
    "@quillsocial/prisma#db-seed": {
      "cache": false,
      "dependsOn": [
        "@quillsocial/prisma#db-migrate"
      ]
    },
    "@quillsocial/prisma#db-up": {
      "cache": false
    },
    "@quillsocial/prisma#dx": {
      "cache": false,
      "dependsOn": [
        "@quillsocial/prisma#db-migrate"
      ]
    },
    "@quillsocial/web#build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "@quillsocial/web#start": {
      "cache": false
    },"build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },"deploy": {
      "cache": false,
      "dependsOn": [
        "@quillsocial/web#build"
      ]
    },
    "clean": {
      "cache": false,
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "post-install": {
      "dependsOn": [],
      "outputs": ["../../node_modules/@prisma/client/**", "../../node_modules/@prisma/admin-client/**"],
      "inputs": ["./schema.prisma", "./prisma/schema.prisma"]
    },
    "@quillsocial/prisma#post-install": {
      "cache": false,
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "start": {
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "type-check": {
      "cache": false
    },
    "type-check:ci": {
      "cache": false
    }
  }
}
